<p id="notice"><%= notice %></p>

<h1>Tipo Mantencions</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_mantencions.each do |tipo_mantencion| %>
      <tr>
        <td><%= tipo_mantencion.nombre %></td>
        <td><%= link_to 'Show', tipo_mantencion %></td>
        <td><%= link_to 'Edit', edit_tipo_mantencion_path(tipo_mantencion) %></td>
        <td><%= link_to 'Destroy', tipo_mantencion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Mantencion', new_tipo_mantencion_path %>
