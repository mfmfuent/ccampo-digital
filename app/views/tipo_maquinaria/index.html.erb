<p id="notice"><%= notice %></p>

<h1>Tipo Maquinaria</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th>Maquinaria</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_maquinaria.each do |tipo_maquinarium| %>
      <tr>
        <td><%= tipo_maquinarium.nombre %></td>
        <td><%= tipo_maquinarium.descripcion %></td>
        <td><%= tipo_maquinarium.maquinaria_id %></td>
        <td><%= link_to 'Show', tipo_maquinarium %></td>
        <td><%= link_to 'Edit', edit_tipo_maquinarium_path(tipo_maquinarium) %></td>
        <td><%= link_to 'Destroy', tipo_maquinarium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Maquinarium', new_tipo_maquinarium_path %>
