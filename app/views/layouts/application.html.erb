<!DOCTYPE html>
<html>
  <head>
    <title>CcampoDigital</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <!-- favicon -->
    <link rel="shortcut icon" href="<%= asset_path 'favicon-32x32.png' %>">
    <!-- Icons -->
    <link href="https://unicons.iconscout.com/release/v3.0.6/css/line.css"  rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <!-- JavaScript Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <!-- Custom fonts for this template-->
    
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
        rel="stylesheet">


    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    
    <%= stylesheet_link_tag 'sb-admin-2', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'sb-admin-2.min', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'style', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'simplebar', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'all', media: 'all', 'data-turbolinks-track': 'reload' %>

    <%= javascript_pack_tag 'sb-admin-2', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'jquery', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'all', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class="text-center">

    <section class="bg-home bg-circle-gradiant d-flex align-items-center">
          <div class="bg-overlay bg-overlay-white"></div>
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="form-signin p-4 bg-white rounded shadow">

                          <p class="notice"><%= notice %></p>
                          <p class="alert"><%= alert %></p>
                          
                          <%= yield %>
    

                        </div>
                      </div>
                  </div>
                  
              </div> <!--end container-->
              
    </section><!--end section-->
    
  </body>
</html>
